{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/styles/*", "/scripts/*", "/games/*", "*.{css,js,png,jpg,gif,svg,ico}"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}